import java.util.*;
import java.io.*;

class Solution {
    public static void main(String[] argh) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt(); // Number of queries
        
        for (int i = 0; i < t; i++) {
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            
            int currentSum = a;
            for (int j = 0; j < n; j++) {
                // Math.pow returns double, so we cast to int
                // Each term adds (2^j * b) to the previous sum
                currentSum += (int) (Math.pow(2, j) * b);
                System.out.print(currentSum + " ");
            }
            // Print a new line after each query series is complete
            System.out.println();
        }
        in.close();
    }
}
